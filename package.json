{
  "name": "bulkan",
  "version": "0.0.0",
  "author": "Alex Aubuchon @A-lxe",
  "bin": {
    "bulkan": "./bin/run"
  },
  "bugs": "https://github.com/A-lxe/bulkan/issues",
  "dependencies": {
    "@oclif/command": "^1.5.15",
    "@oclif/config": "^1.13.0",
    "@oclif/plugin-help": "^2.2.0",
    "fast-glob": "^3.0.1",
    "filesize": "^4.1.2",
    "node-module-resolution": "0.0.3",
    "tslib": "^1.10.0"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^0.1.3",
    "@oclif/dev-cli": "^1.22.0",
    "@oclif/test": "^1.2.4",
    "@oclif/tslint": "^3.1.1",
    "@types/chai": "^4.1.7",
    "@types/filesize": "^4.1.0",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.0.10",
    "chai": "^4.2.0",
    "globby": "^9.2.0",
    "gts": "^1.0.0",
    "mocha": "^6.1.4",
    "nyc": "^14.1.1",
    "source-map-support": "^0.5.12",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "typescript": "^3.5.2"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/dist",
    "npm-shrinkwrap.json",
    "oclif.manifest.json"
  ],
  "homepage": "https://github.com/A-lxe/bulkan",
  "keywords": [],
  "license": "Apache-2.0",
  "main": "dist/index.js",
  "mocha": {
    "require": [
      "ts-node/register",
      "source-map-support/register"
    ],
    "extension": "ts",
    "reporter": "spec",
    "recursive": true
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "all": true
  },
  "oclif": {
    "commands": "./dist/commands",
    "bin": "bulkan",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "A-lxe/bulkan",
  "scripts": {
    "check": "gts check",
    "compile": "tsc",
    "fix": "gts fix",
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "npm run check",
    "prepack": "rm -rf dist && tsc -b && oclif-dev manifest && oclif-dev readme",
    "prepare": "npm run compile",
    "test": "nyc mocha --forbid-only",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "dist/index.d.ts"
}
